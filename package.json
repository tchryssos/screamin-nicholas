{
  "name": "screamin-nicholas",
  "version": "1.0.0",
  "description": "A discord bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc && uglifyjs dist/index.js -m -c -o dist/index.js",
    "start": "npm run build && node --experimental-json-modules ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2",
    "uglify-js": "^3.14.1"
  },
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write"
    ],
    "*.{ts}": [
      "eslint --fix"
    ]
  }
}
