{
  "name": "screamin-nicholas",
  "version": "1.2.0",
  "description": "A discord bot that can play audio from Youtube in your voice channels",
  "type": "module",
  "nodeVersion": "16.6.0",
  "scripts": {
    "build": "rm -rf dist && npx tsc && cp package.json package-lock.json .env dist",
    "dev": "npm run build && node dist/index.js",
    "start": "node index.js",
    "deployCommands": "npm run build && node dist/commands/utils/deployCommands.js"
  },
  "author": "troy@troychryssos.com",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/lodash.isempty": "^4.4.6",
    "@types/lodash.shuffle": "^4.2.6",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/builders": "^1.0.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.0.0",
    "@discordjs/voice": "^0.11.0",
    "discord-api-types": "^0.36.3",
    "discord.js": "^14.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash.isempty": "^4.4.0",
    "lodash.shuffle": "^4.2.0",
    "opusscript": "^0.0.8",
    "spotify-web-api-node": "^5.0.2",
    "ytdl-core": "^4.11.0",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write"
    ],
    "*.{ts}": [
      "eslint --fix"
    ]
  }
}
